    }
   ],
   "source": [
    "#This depends on Imax, b (from the equation from Goldhaber)\n",
    "Peak_flux_error_rel = np.sqrt((perr[0]/Imax)**2 + (1+(perr[2]/popt_binned[2]))**2)\n",
    "Peak_flux_error_abs = peak_flux * Peak_flux_error_rel\n",
    "print('Peak Flux', peak_flux)\n",
    "print('Peak Flux Relative Error',Peak_flux_error_rel)\n",
    "print('Peak Flux Absolute Error', Peak_flux_error_abs)"
   ]
