     ]
    }
   ],
   "source": [
    "#This only depend on apparent and absolute magnitude values\n",
    "dist_mod_error = np.sqrt((M_B_error)**2 + (m_b_error)**2)\n",
    "\n",
    "print('Distance Modulus', dist_mod)\n",
    "print('Distance Modulus Error', dist_mod_error)\n"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 6. Final Distance Error"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 72,
   "metadata": {
    "collapsed": false
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Final Distance in MPC 6.58902084515\n",
      "Distance Error 1.62235982998\n"
     ]
    }
   ],
   "source": [
    "#This only depends on distance modulus\n",
    "x = np.exp(np.log(10) * (peak_mag - M_B_corrected + 5)/(5)) * 0.2 * np.log(10) * m_b_error\n",
    "y = np.exp(np.log(10) * (peak_mag - M_B_corrected + 5)/(5)) * 0.2 * np.log(10) * M_B_error\n",
    "\n",
    "dist_err = np.sqrt((x)**2 + (y)**2)/(1e6)\n",
    "\n",
    "\n",
    "print('Final Distance in MPC', distance)\n",
    "print('Distance Error', dist_err)"
   ]
